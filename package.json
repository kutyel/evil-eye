{
  "name": "evil-eye",
  "description": "Small evil eye to see the inner parts of js...",
  "homepage": "https://documentup.com/kutyel/evil-eye",
  "version": "0.0.0",
  "main": "index.js",
  "author": {
    "email": "flavio.corparios@ext.soprasteria.com",
    "name": "Flavio Corpa",
    "url": "https://flaviocorpa.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kutyel/evil-eye.git"
  },
  "bugs": {
    "url": "https://github.com/kutyel/evil-eye/issues"
  },
  "keywords": [
    "eye",
    "fp",
    "js",
    "log",
    "logger"
  ],
  "dependencies": {
    "eyes": "~0.1.8"
  },
  "devDependencies": {
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "jest": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "^11.0.0",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "standard-markdown README.md && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin,scripts}/**/*.js --single-quote --print-width 100",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "license": "MIT",
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 83,
        "lines": 100,
        "statements": 92
      }
    },
    "testEnvironment": "node"
  },
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ],
    "*.md": [
      "standard-markdown",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
